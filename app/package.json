{
  "name": "docker-webpack-express-react",
  "version": "1.0.0",
  "description": "A web application boilerplate based on Docker, Express, Webpack, and React",
  "main": "src/index.js",
  "scripts": {
    "start": "BABEL_ENV=server NODE_ENV=development node -r babel-register src/index.js",
    "babel:build": "babel src --out-dir dist",
    "webpack:build": "webpack --config ./webpack.config.js",
    "test": "jest --coverage",
    "lint": "eslint ."
